<Window x:Class="Setup.EndingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Setup"
        mc:Ignorable="d"
        Title="Success" Height="148" Width="565">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="201*"/>
            <ColumnDefinition Width="35*"/>
        </Grid.ColumnDefinitions>
        <Label Content="You have successfully finished setup" HorizontalAlignment="Left" Margin="103,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.122,-0.881" Foreground="#FF23BDBF" FontSize="20" Grid.ColumnSpan="2" Width="401"/>
        <Button Content="Finish" HorizontalAlignment="Left" Margin="466,69,0,0" VerticalAlignment="Top" RenderTransformOrigin="3.916,1.96" Width="76" Click="Button_Click" Grid.ColumnSpan="2"/>
        <CheckBox Name="SetupCheckbox" IsChecked="True" Content="Clean, Build and Open project in visual studio" HorizontalAlignment="Left" Margin="34,72,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
